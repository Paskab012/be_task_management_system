{
  "name": "be_task_management_system",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:create": "cross-env ts-node -r tsconfig-paths/register node_modules/sequelize-cli/lib/sequelize db:create",
    "db:drop": "cross-env ts-node -r tsconfig-paths/register node_modules/sequelize-cli/lib/sequelize db:drop",
    "db:migrate": "cross-env NODE_ENV=${NODE_ENV:-development} ts-node -r tsconfig-paths/register --transpile-only node_modules/.bin/sequelize-cli db:migrate",
    "db:migrate:prod": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register --transpile-only node_modules/.bin/sequelize-cli db:migrate",
    "db:migrate:undo": "cross-env NODE_ENV=${NODE_ENV:-development} ts-node -r tsconfig-paths/register --transpile-only node_modules/.bin/sequelize-cli db:migrate:undo",
    "db:seed": "cross-env NODE_ENV=${NODE_ENV:-development} ts-node -r tsconfig-paths/register --transpile-only node_modules/.bin/sequelize-cli db:seed:all",
    "db:seed:prod": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register --transpile-only node_modules/.bin/sequelize-cli db:seed:all",
    "db:seed:undo": "cross-env NODE_ENV=${NODE_ENV:-development} ts-node -r tsconfig-paths/register --transpile-only node_modules/.bin/sequelize-cli db:seed:undo",
    "db:reset": "yarn db:drop && yarn db:create && yarn db:migrate && yarn db:seed",
    "db:reset:prod": "yarn db:drop && yarn db:create && yarn db:migrate:prod && yarn db:seed:prod",
    "migrate:all": "yarn build && yarn db:migrate && yarn db:seed",
    "migrate:all:prod": "yarn build && yarn db:migrate:prod && yarn db:seed:prod",
    "docker:dev": "docker-compose -f docker-compose.yml up --build",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up --build",
    "docker:down": "docker-compose down",
    "docker:clean": "docker-compose down -v --rmi all"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.1.6",
    "@nestjs/sequelize": "^11.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/websockets": "^11.1.6",
    "bcryptjs": "^3.0.2",
    "cache-manager": "^7.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "module-alias": "^2.2.3",
    "nest-winston-module": "^0.1.3",
    "node-mailjet": "^6.0.9",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "redis": "^5.8.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.6",
    "@types/bcryptjs": "^3.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.15.5",
    "@types/sequelize": "^6.12.0",
    "@types/supertest": "^6.0.2",
    "cross-env": "^10.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.5",
    "prettier": "^3.4.2",
    "sequelize-cli": "^6.6.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "_moduleAliases": {
    "@": "dist",
    "@common": "dist/common",
    "@database": "dist/database",
    "@modules": "dist/modules",
    "@constants": "dist/constants",
    "@utils": "dist/utils",
    "@interfaces": "dist/interfaces",
    "@swagger": "dist/swagger"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
